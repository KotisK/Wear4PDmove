@prefix : <http://w3id.org/Wear4PDmove/onto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://w3id.org/Wear4PDmove/onto#> .

<http://w3id.org/Wear4PDmove/onto> rdf:type owl:Ontology ;
                                    owl:versionIRI <http://w3id.org/Wear4PDmove/onto/1.0.0> ;
                                    owl:imports <http://www.case.edu/PMDO> ,
                                                <http://www.w3.org/ns/sosa/> ,
                                                <https://dahcc.idlab.ugent.be/Ontology/SensorsAndWearables/> ;
                                    <http://purl.org/dc/terms/contributor> "Nikolaos Zafeiropoulos" ,
                                                                           "Pavlos Bitilis" ;
                                    <http://purl.org/dc/terms/created> "05-05-2023" ;
                                    <http://purl.org/dc/terms/creator> "Konstantinos Kotis" ;
                                    <http://purl.org/dc/terms/description> "Wear4PDmove: an ontology for knowledge-based personalized health monitoring of PD patients" ;
                                    <http://purl.org/dc/terms/license> "CC0" ;
                                    rdfs:comment "The designed Wear4PDmoveOnto aims to semantically integrate heterogeneous data such as dynamic/stream data  (collected from wearables) and static/historic data (stored in health databases), in order to represent personal health knowledge in the form of a KG, supporting health apps’ reasoning capabilities for high-level events recognition (e.g., ‘missing dose’ or  ‘patient fall’ event)." ;
                                    rdfs:label "Wear4PDmoveOnto"@en ;
                                    owl:versionInfo "v1.0.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/Wear4PDmove/onto#consistsOf
:consistsOf rdf:type owl:ObjectProperty ;
            rdfs:domain :PersonalHealthRecord .


###  http://w3id.org/Wear4PDmove/onto#hasDosageTime
:hasDosageTime rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <https://saref.etsi.org/core/hasTime> ;
               rdfs:domain :DailyDosagePlan ;
               rdfs:range :DosageTime .


###  http://w3id.org/Wear4PDmove/onto#hasPersonalHealthRecord
:hasPersonalHealthRecord rdf:type owl:ObjectProperty ;
                         rdfs:domain :Patient ;
                         rdfs:range :PersonalHealthRecord ;
                         rdfs:comment "" .


###  http://w3id.org/Wear4PDmove/onto#isPerformedBy
:isPerformedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf :performs ;
               rdfs:label "isPerformedBy" .


###  http://w3id.org/Wear4PDmove/onto#performs
:performs rdf:type owl:ObjectProperty ;
          rdfs:domain :Patient ;
          rdfs:range <http://www.case.edu/MDO#ActivitiesofDailyLiving> ;
          rdfs:comment "human (patient) performs an action (ADL)" ;
          rdfs:label "performs" .


#################################################################
#    Data properties
#################################################################

###  http://w3id.org/Wear4PDmove/onto#ageOfPatient
:ageOfPatient rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:range xsd:integer .


###  http://w3id.org/Wear4PDmove/onto#bodyMassIndex
:bodyMassIndex rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:range xsd:decimal .


###  http://w3id.org/Wear4PDmove/onto#diseaseDuration
:diseaseDuration rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:range xsd:integer .


###  http://w3id.org/Wear4PDmove/onto#hasAkinesia
:hasAkinesia rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:range xsd:boolean .


###  http://w3id.org/Wear4PDmove/onto#hasAnxiety
:hasAnxiety rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:range xsd:boolean .


###  http://w3id.org/Wear4PDmove/onto#hasBradykinisiaOfUpperLimp
:hasBradykinisiaOfUpperLimp rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf owl:topDataProperty ;
                            rdfs:range xsd:boolean .


###  http://w3id.org/Wear4PDmove/onto#hasDepression
:hasDepression rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:range xsd:boolean .


###  http://w3id.org/Wear4PDmove/onto#hasDysphagia
:hasDysphagia rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:range xsd:boolean .


###  http://w3id.org/Wear4PDmove/onto#hasFatigue
:hasFatigue rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:range xsd:boolean .


###  http://w3id.org/Wear4PDmove/onto#hasFreezingOfGait
:hasFreezingOfGait rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf owl:topDataProperty ;
                   rdfs:range xsd:boolean .


###  http://w3id.org/Wear4PDmove/onto#hasPosturalInstability
:hasPosturalInstability rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf owl:topDataProperty ;
                        rdfs:range xsd:boolean .


###  http://w3id.org/Wear4PDmove/onto#hasRigidity
:hasRigidity rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:range xsd:boolean .


###  http://w3id.org/Wear4PDmove/onto#hasSleepDisturbances
:hasSleepDisturbances rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdfs:range xsd:boolean .


###  http://w3id.org/Wear4PDmove/onto#hasTremor
:hasTremor rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:range xsd:boolean .


###  http://w3id.org/Wear4PDmove/onto#heartRate
:heartRate rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:range xsd:decimal .


###  http://w3id.org/Wear4PDmove/onto#levelHoehnYahrStage
:levelHoehnYahrStage rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdfs:range xsd:integer .


###  http://w3id.org/Wear4PDmove/onto#obsAfterDosing
:obsAfterDosing rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:boolean ;
                rdfs:comment "Check if observation is taken 30 min after the last dosage of dosage plan" .


###  http://w3id.org/Wear4PDmove/onto#scoreUPDRS
:scoreUPDRS rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:range xsd:decimal .


###  http://w3id.org/Wear4PDmove/onto#sendNotification
:sendNotification rdf:type owl:DatatypeProperty ;
                  rdfs:domain :AlertingNotification ;
                  rdfs:range xsd:boolean .


###  https://dahcc.idlab.ugent.be/Ontology/SensorsAndWearables/hasAxis
<https://dahcc.idlab.ugent.be/Ontology/SensorsAndWearables/hasAxis> rdf:type owl:DatatypeProperty .


###  https://saref.etsi.org/core/hasTimestamp
<https://saref.etsi.org/core/hasTimestamp> rdfs:label "dose time" .


#################################################################
#    Classes
#################################################################

###  http://w3id.org/Wear4PDmove/onto#AlertingNotification
:AlertingNotification rdf:type owl:Class ;
                      rdfs:subClassOf <https://saref.etsi.org/core/ActuatingFunction> ;
                      rdfs:comment "Alerting Notification Function for event" ;
                      rdfs:label "Alerting Notification Function" .


###  http://w3id.org/Wear4PDmove/onto#DailyDosagePlan
:DailyDosagePlan rdf:type owl:Class ;
                 rdfs:subClassOf :MedicalPrescription ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasDosageTime ;
                                   owl:someValuesFrom :DosageTime
                                 ] ;
                 rdfs:comment "Daily dosage plan for patient (4 times one day)" ;
                 rdfs:label "Daily Dosage Plan" .


###  http://w3id.org/Wear4PDmove/onto#Diagnosis
:Diagnosis rdf:type owl:Class ;
           rdfs:subClassOf :PersonalHealthRecord ;
           rdfs:comment "diagnosis for patient in one appointment" ;
           rdfs:label "Diagnosis" .


###  http://w3id.org/Wear4PDmove/onto#DosageTime
:DosageTime rdf:type owl:Class ;
            rdfs:subClassOf <https://saref.etsi.org/core/Time> ;
            rdfs:comment "Dosage time on datetime" ;
            rdfs:label "Dosage Time (time in one day)" .


###  http://w3id.org/Wear4PDmove/onto#MedicalPrescription
:MedicalPrescription rdf:type owl:Class ;
                     rdfs:subClassOf :PersonalHealthRecord ;
                     rdfs:comment "Medical Prescription for Patient" ;
                     rdfs:label "Medical Prescription" .


###  http://w3id.org/Wear4PDmove/onto#NotifyDoctor
:NotifyDoctor rdf:type owl:Class ;
              rdfs:subClassOf :AlertingNotification ;
              rdfs:comment "Notify doctor for event (alert notification)" ;
              rdfs:label "Notify Doctor" .


###  http://w3id.org/Wear4PDmove/onto#PD_Patient
:PD_Patient rdf:type owl:Class ;
            rdfs:subClassOf :Patient ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasPersonalHealthRecord ;
                              owl:someValuesFrom :PersonalHealthRecord
                            ] ;
            rdfs:comment "PD patient" ;
            rdfs:label "PD Patient" .


###  http://w3id.org/Wear4PDmove/onto#PDpatientFalllingEventObservation
:PDpatientFalllingEventObservation rdf:type owl:Class ;
                                   rdfs:subClassOf :RecognizedEvent ;
                                   rdfs:comment "Falling event observation for PD Patient" ;
                                   rdfs:label "PDpatient Fallling Event Observation" .


###  http://w3id.org/Wear4PDmove/onto#PDpatientMissingDoseEventObservation
:PDpatientMissingDoseEventObservation rdf:type owl:Class ;
                                      rdfs:subClassOf :RecognizedEvent ;
                                      rdfs:comment "Missing dose event Observation for PD Patient" ;
                                      rdfs:label "PDpatient Missing Dose Event Observation" .


###  http://w3id.org/Wear4PDmove/onto#Patient
:Patient rdf:type owl:Class ;
         rdfs:subClassOf <http://schema.org/Person> ;
         rdfs:comment "Any kind of patient" ;
         rdfs:label "Patient" .


###  http://w3id.org/Wear4PDmove/onto#PersonalHealthRecord
:PersonalHealthRecord rdf:type owl:Class ;
                      rdfs:comment "Personal Health Record of a patient" ;
                      rdfs:label "Personal Health Record" .


###  http://w3id.org/Wear4PDmove/onto#RecognizedEvent
:RecognizedEvent rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.w3.org/ns/sosa/Observation> ;
                 rdfs:comment "Recognized event for one PD Patient" ;
                 rdfs:label "PD patient recognized event observation" .


###  http://w3id.org/Wear4PDmove/onto#SketchingActivity
:SketchingActivity rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.case.edu/MDO#ActivitiesofDailyLiving> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <https://saref.etsi.org/core/isMeasuredByDevice> ;
                                     owl:someValuesFrom :SmartWatch
                                   ] ;
                   rdfs:comment "An activity followed by the patient during his free time, using his hands (sketching)."@en ;
                   rdfs:label "Sketching Activity"@en .


###  http://w3id.org/Wear4PDmove/onto#SmartWatch
:SmartWatch rdf:type owl:Class ;
            rdfs:subClassOf <https://saref.etsi.org/saref4wear/OnBodyWearable> ;
            rdfs:comment "An off-the-shelf smart watch with sensors able to measure movement"@en ;
            rdfs:label "Smart Watch"@en .


###  http://w3id.org/Wear4PDmove/onto#WalkingActivity
:WalkingActivity rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.case.edu/MDO#ActivitiesofDailyLiving> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <https://saref.etsi.org/core/isMeasuredByDevice> ;
                                   owl:someValuesFrom :SmartWatch
                                 ] ;
                 rdfs:comment "An activity followed by the patient during his free time, using his hands (walking)."@en ;
                 rdfs:label "Walking Activity"@en .


###  http://www.case.edu/MDO#ActivitiesofDailyLiving
<http://www.case.edu/MDO#ActivitiesofDailyLiving> rdf:type owl:Class .


###  http://www.case.edu/MDO#Tremor
<http://www.case.edu/MDO#Tremor> rdf:type owl:Class .


###  http://www.case.edu/PMDO#BradykinesiaofUpperLimb
<http://www.case.edu/PMDO#BradykinesiaofUpperLimb> rdf:type owl:Class .


###  http://www.w3.org/ns/sosa/FeatureOfInterest
<http://www.w3.org/ns/sosa/FeatureOfInterest> owl:equivalentClass <https://saref.etsi.org/core/FeatureOfInterest> .


#################################################################
#    Individuals
#################################################################

###  http://w3id.org/Wear4PDmove/onto#Acc22-sensor
:Acc22-sensor rdf:type owl:NamedIndividual ,
                       <https://dahcc.idlab.ugent.be/Ontology/SensorsAndWearables/Accelerometer> ;
              <http://www.w3.org/ns/sosa/observes> :Wearable_Acceleration_for_PD_movement ;
              rdfs:label "Samsung Sensor Accelerometer 22" .


###  http://w3id.org/Wear4PDmove/onto#BradykinisiaUpperLimpForPDpatient
:BradykinisiaUpperLimpForPDpatient rdf:type owl:NamedIndividual ,
