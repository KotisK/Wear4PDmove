<?xml version="1.0"?>
<rdf:RDF xmlns="http://i-lab.aegean.gr/ontologies/Wear4PDmoveOnto"
     xml:base="http://i-lab.aegean.gr/ontologies/Wear4PDmoveOnto"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:core="https://saref.etsi.org/core/"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:onto="http://w3id.org/Wear4PDmove/onto#"
     xmlns:pmdo="http://www.case.edu/PMDO"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:sosa="http://www.w3.org/ns/sosa/"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:dahcc="https://dahcc.idlab.ugent.be/Ontology/SensorsAndWearables/"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="http://schema.org/"
     xmlns:Wear4PDmove="http://i-lab.aegean.gr/kotis/Ontologies/Wear4PDmove#"
     xmlns:Wear4PDmoveOnto="http://i-lab.aegean.gr/ontologies/Wear4PDmoveOnto#">
    <owl:Ontology rdf:about="http://w3id.org/Wear4PDmove/onto">
        <owl:versionIRI rdf:resource="http://w3id.org/Wear4PDmove/onto/1.0.0"/>
        <owl:imports rdf:resource="http://www.case.edu/PMDO"/>
        <owl:imports rdf:resource="http://www.w3.org/ns/sosa/"/>
        <owl:imports rdf:resource="https://dahcc.idlab.ugent.be/Ontology/SensorsAndWearables/"/>
        <terms:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nikolaos Zafeiropoulos</terms:contributor>
        <terms:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pavlos Bitilis</terms:contributor>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#string">05-05-2023</terms:created>
        <terms:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Konstantinos Kotis</terms:creator>
        <terms:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wear4PDmove: an ontology for knowledge-based personalized health monitoring of PD patients</terms:description>
        <terms:license rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CC0</terms:license>
        <rdfs:comment>The designed Wear4PDmoveOnto aims to semantically integrate heterogeneous data such as dynamic/stream data  (collected from wearables) and static/historic data (stored in health databases), in order to represent personal health knowledge in the form of a KG, supporting health apps’ reasoning capabilities for high-level events recognition (e.g., ‘missing dose’ or  ‘patient fall’ event).</rdfs:comment>
        <rdfs:label xml:lang="en">Wear4PDmoveOnto</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">v1.0.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/Wear4PDmove/onto#consistsOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/Wear4PDmove/onto#consistsOf">
        <rdfs:domain rdf:resource="http://w3id.org/Wear4PDmove/onto#PersonalHealthRecord"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#hasDosageTime -->

    <owl:ObjectProperty rdf:about="http://w3id.org/Wear4PDmove/onto#hasDosageTime">
        <rdfs:subPropertyOf rdf:resource="https://saref.etsi.org/core/hasTime"/>
        <rdfs:domain rdf:resource="http://w3id.org/Wear4PDmove/onto#DailyDosagePlan"/>
        <rdfs:range rdf:resource="http://w3id.org/Wear4PDmove/onto#DosageTime"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#hasPersonalHealthRecord -->

    <owl:ObjectProperty rdf:about="http://w3id.org/Wear4PDmove/onto#hasPersonalHealthRecord">
        <rdfs:domain rdf:resource="http://w3id.org/Wear4PDmove/onto#Patient"/>
        <rdfs:range rdf:resource="http://w3id.org/Wear4PDmove/onto#PersonalHealthRecord"/>
        <rdfs:comment></rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#isPerformedBy -->

    <owl:ObjectProperty rdf:about="http://w3id.org/Wear4PDmove/onto#isPerformedBy">
        <owl:inverseOf rdf:resource="http://w3id.org/Wear4PDmove/onto#performs"/>
        <rdfs:label>isPerformedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#performs -->

    <owl:ObjectProperty rdf:about="http://w3id.org/Wear4PDmove/onto#performs">
        <rdfs:domain rdf:resource="http://w3id.org/Wear4PDmove/onto#Patient"/>
        <rdfs:range rdf:resource="http://www.case.edu/MDO#ActivitiesofDailyLiving"/>
        <rdfs:comment>human (patient) performs an action (ADL)</rdfs:comment>
        <rdfs:label>performs</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/Wear4PDmove/onto#ageOfPatient -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#ageOfPatient">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#bodyMassIndex -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#bodyMassIndex">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#diseaseDuration -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#diseaseDuration">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#hasAkinesia -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#hasAkinesia">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#hasAnxiety -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#hasAnxiety">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#hasBradykinisiaOfUpperLimp -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#hasBradykinisiaOfUpperLimp">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#hasDepression -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#hasDepression">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#hasDysphagia -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#hasDysphagia">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#hasFatigue -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#hasFatigue">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#hasFreezingOfGait -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#hasFreezingOfGait">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#hasPosturalInstability -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#hasPosturalInstability">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#hasRigidity -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#hasRigidity">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#hasSleepDisturbances -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#hasSleepDisturbances">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#hasTremor -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#hasTremor">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#heartRate -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#heartRate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#levelHoehnYahrStage -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#levelHoehnYahrStage">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#obsAfterDosing -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#obsAfterDosing">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Check if observation is taken 30 min after the last dosage of dosage plan</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#scoreUPDRS -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#scoreUPDRS">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/Wear4PDmove/onto#sendNotification -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/Wear4PDmove/onto#sendNotification">
        <rdfs:domain rdf:resource="http://w3id.org/Wear4PDmove/onto#AlertingNotification"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- https://dahcc.idlab.ugent.be/Ontology/SensorsAndWearables/hasAxis -->

    <owl:DatatypeProperty rdf:about="https://dahcc.idlab.ugent.be/Ontology/SensorsAndWearables/hasAxis"/>
    


    <!-- https://saref.etsi.org/core/hasTimestamp -->

    <rdf:Description rdf:about="https://saref.etsi.org/core/hasTimestamp">
        <rdfs:label>dose time</rdfs:label>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/Wear4PDmove/onto#AlertingNotification -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#AlertingNotification">
        <rdfs:subClassOf rdf:resource="https://saref.etsi.org/core/ActuatingFunction"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alerting Notification Function for event</rdfs:comment>
        <rdfs:label>Alerting Notification Function</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#DailyDosagePlan -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#DailyDosagePlan">
        <rdfs:subClassOf rdf:resource="http://w3id.org/Wear4PDmove/onto#MedicalPrescription"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/Wear4PDmove/onto#hasDosageTime"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/Wear4PDmove/onto#DosageTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Daily dosage plan for patient (4 times one day)</rdfs:comment>
        <rdfs:label>Daily Dosage Plan</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#Diagnosis -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#Diagnosis">
        <rdfs:subClassOf rdf:resource="http://w3id.org/Wear4PDmove/onto#PersonalHealthRecord"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">diagnosis for patient in one appointment</rdfs:comment>
        <rdfs:label>Diagnosis</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#DosageTime -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#DosageTime">
        <rdfs:subClassOf rdf:resource="https://saref.etsi.org/core/Time"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dosage time on datetime</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dosage Time (time in one day)</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#MedicalPrescription -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#MedicalPrescription">
        <rdfs:subClassOf rdf:resource="http://w3id.org/Wear4PDmove/onto#PersonalHealthRecord"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Medical Prescription for Patient</rdfs:comment>
        <rdfs:label>Medical Prescription</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#NotifyDoctor -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#NotifyDoctor">
        <rdfs:subClassOf rdf:resource="http://w3id.org/Wear4PDmove/onto#AlertingNotification"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Notify doctor for event (alert notification)</rdfs:comment>
        <rdfs:label>Notify Doctor</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#PD_Patient -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#PD_Patient">
        <rdfs:subClassOf rdf:resource="http://w3id.org/Wear4PDmove/onto#Patient"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/Wear4PDmove/onto#hasPersonalHealthRecord"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/Wear4PDmove/onto#PersonalHealthRecord"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>PD patient</rdfs:comment>
        <rdfs:label>PD Patient</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#PDpatientFalllingEventObservation -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#PDpatientFalllingEventObservation">
        <rdfs:subClassOf rdf:resource="http://w3id.org/Wear4PDmove/onto#RecognizedEvent"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Falling event observation for PD Patient</rdfs:comment>
        <rdfs:label>PDpatient Fallling Event Observation</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#PDpatientMissingDoseEventObservation -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#PDpatientMissingDoseEventObservation">
        <rdfs:subClassOf rdf:resource="http://w3id.org/Wear4PDmove/onto#RecognizedEvent"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Missing dose event Observation for PD Patient</rdfs:comment>
        <rdfs:label>PDpatient Missing Dose Event Observation</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#Patient -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#Patient">
        <rdfs:subClassOf rdf:resource="http://schema.org/Person"/>
        <rdfs:comment>Any kind of patient</rdfs:comment>
        <rdfs:label>Patient</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#PersonalHealthRecord -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#PersonalHealthRecord">
        <rdfs:comment>Personal Health Record of a patient</rdfs:comment>
        <rdfs:label>Personal Health Record</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#RecognizedEvent -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#RecognizedEvent">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Recognized event for one PD Patient</rdfs:comment>
        <rdfs:label>PD patient recognized event observation</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#SketchingActivity -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#SketchingActivity">
        <rdfs:subClassOf rdf:resource="http://www.case.edu/MDO#ActivitiesofDailyLiving"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://saref.etsi.org/core/isMeasuredByDevice"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/Wear4PDmove/onto#SmartWatch"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An activity followed by the patient during his free time, using his hands (sketching).</rdfs:comment>
        <rdfs:label xml:lang="en">Sketching Activity</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#SmartWatch -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#SmartWatch">
        <rdfs:subClassOf rdf:resource="https://saref.etsi.org/saref4wear/OnBodyWearable"/>
        <rdfs:comment xml:lang="en">An off-the-shelf smart watch with sensors able to measure movement</rdfs:comment>
        <rdfs:label xml:lang="en">Smart Watch</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/Wear4PDmove/onto#WalkingActivity -->

    <owl:Class rdf:about="http://w3id.org/Wear4PDmove/onto#WalkingActivity">
        <rdfs:subClassOf rdf:resource="http://www.case.edu/MDO#ActivitiesofDailyLiving"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://saref.etsi.org/core/isMeasuredByDevice"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/Wear4PDmove/onto#SmartWatch"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An activity followed by the patient during his free time, using his hands (walking).</rdfs:comment>
        <rdfs:label xml:lang="en">Walking Activity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.case.edu/MDO#ActivitiesofDailyLiving -->

    <owl:Class rdf:about="http://www.case.edu/MDO#ActivitiesofDailyLiving"/>
    


    <!-- http://www.case.edu/MDO#Tremor -->

    <owl:Class rdf:about="http://www.case.edu/MDO#Tremor"/>
    


    <!-- http://www.case.edu/PMDO#BradykinesiaofUpperLimb -->

    <owl:Class rdf:about="http://www.case.edu/PMDO#BradykinesiaofUpperLimb"/>
    


    <!-- http://www.w3.org/ns/sosa/FeatureOfInterest -->

    <rdf:Description rdf:about="http://www.w3.org/ns/sosa/FeatureOfInterest">
        <owl:equivalentClass rdf:resource="https://saref.etsi.org/core/FeatureOfInterest"/>
    </rdf:Description>
    


    <!-- https://saref.etsi.org/core/FeatureOfInterest -->

    <rdf:Description rdf:about="https://saref.etsi.org/core/FeatureOfInterest"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/Wear4PDmove/onto#Acc22-sensor -->

    <owl:NamedIndividual rdf:about="http://w3id.org/Wear4PDmove/onto#Acc22-sensor">
        <rdf:type rdf:resource="https://dahcc.idlab.ugent.be/Ontology/SensorsAndWearables/Accelerometer"/>
        <sosa:observes rdf:resource="http://w3id.org/Wear4PDmove/onto#Wearable_Acceleration_for_PD_movement"/>
